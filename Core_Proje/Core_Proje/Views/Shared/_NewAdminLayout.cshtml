            <!-- content-wrapper ends -->
            <!-- partial:../../partials/_footer.html -->
            <footer class="footer">
                <div class="d-sm-flex justify-content-center justify-content-sm-between">
                    <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright © Asrın Ziya Hocaoğlu</span>
                </div>
            </footer>
            <!-- partial -->
        </div>
        
    </div>
   
</div>
<!--Scripts-->

@await Html.PartialAsync("/Views/Admin/_ScriptsPartial.cshtml")

<!--Scripts-->

</body>
</html>
