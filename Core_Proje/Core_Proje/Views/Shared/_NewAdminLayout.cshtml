
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<!-- Head -->

@await Html.PartialAsync("/Views/Admin/_HeadPartial.cshtml")

<!-- Head -->
<body>
<div class="container-scroller">
    <!-- SideBar -->

    @await Html.PartialAsync("/Views/Admin/NewSideBar.cshtml")

    <!-- SideBar -->

    <div class="container-fluid page-body-wrapper">
        <!-- NavBar -->

        @await Html.PartialAsync("/Views/Admin/_NavbarPartial.cshtml")

        <!-- NavBar -->
